{
  "name": "sveltekit-template",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build:site": "vite build",
    "build:lib": "svelte-kit sync && svelte-package",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "pret": "prettier --plugin-search-dir=. --check .",
    "pret:fix": "prettier --plugin-search-dir=. --write .",
    "autoupdate": "tsx src/autoupdate.cts"
  },
  "devDependencies": {
    "@iconify-json/carbon": "^1.1.18",
    "@iconify-json/ri": "^1.1.10",
    "@sveltejs/adapter-static": "^2.0.2",
    "@sveltejs/kit": "^1.20.5",
    "@sveltejs/package": "^2.1.0",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "@unocss/extractor-svelte": "^0.53.3",
    "@unocss/preset-attributify": "^0.53.3",
    "@unocss/preset-icons": "^0.53.3",
    "@unocss/preset-uno": "^0.53.3",
    "@unocss/reset": "^0.53.3",
    "@unocss/transformer-directives": "^0.53.3",
    "@unocss/transformer-variant-group": "^0.53.3",
    "@unocss/vite": "^0.53.3",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-svelte": "^2.31.0",
    "glob": "^10.3.0",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "svelte": "^4.0.0",
    "svelte-check": "^3.4.4",
    "svelte-eslint-parser": "^0.31.0",
    "svelte-preprocess-import-assets": "^1.0.1",
    "svoast": "^2.2.0",
    "tslib": "^2.5.3",
    "tsx": "^3.12.7",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vite-imagetools": "^5.0.4"
  },
  "dependencies": {
    "@fontsource/noto-sans-sc": "^5.0.3",
    "photoswipe": "^5.3.7"
  }
}
